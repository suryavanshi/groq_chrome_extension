// manifest.json
{
  "manifest_version": 2,
  "name": "LLM Page Scraper",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "https://api.groq.com/"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}